Private Sub Form_Unload(Cancel As Integer)
PlaySound 14
ConsoleWrite "ShutDown NORMAL"
If MsgBox(Title:="Opravdu opustit program ???", prompt:="Myslíš to VÁŽNÌ ???", Buttons:=vbYesNo) = vbYes Then
  Select Case ReadIniFile("ini\main.ini", "Common", "ShowReklama")
  Case "1"
    ConsoleWrite "Value 'Reklama' is TRUE"
    MCIfrm.AMOVIE1.Stop
    ReklFRM.Show
  Case "0"
    ConsoleWrite "Value 'Reklama' is FALSE"
    MCIfrm.AMOVIE1.Stop
    ConsoleWrite "ShutDown NOW"
    WriteLOG
    End
  End Select
 Else
    ConsoleWrite "CANCELED"
    Cancel = True
    End If
End Sub







